# Notion Integration Configuration
# Copy this file to .env.local and fill in your actual values

# Your Notion Integration Token
# Get this from: https://www.notion.so/my-integrations
VITE_NOTION_TOKEN=your_notion_integration_token_here

# Your Notion Database ID  
# This is the ID of your blog posts database in Notion
VITE_NOTION_DATABASE_ID=your_notion_database_id_here

# Optional: Enable/disable Notion integration
VITE_USE_NOTION=false
